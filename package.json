{
  "name": "betteraiengine-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for BetterAI Engine - AI-enhanced predictions for Polymarket",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm --filter @betteraiengine/cli dev",
    "build": "pnpm --filter @betteraiengine/core build && pnpm --filter @betteraiengine/cli build",
    "build:core": "pnpm --filter @betteraiengine/core build",
    "build:cli": "pnpm --filter @betteraiengine/cli build",
    "db:generate": "pnpm --filter @betteraiengine/core db:generate",
    "db:migrate": "pnpm --filter @betteraiengine/core db:migrate",
    "db:push": "pnpm --filter @betteraiengine/core db:push",
    "db:studio": "pnpm --filter @betteraiengine/core db:studio"
  },
  "keywords": [
    "polymarket",
    "ai",
    "predictions",
    "monorepo"
  ],
  "author": "",
  "license": "MIT"
}
